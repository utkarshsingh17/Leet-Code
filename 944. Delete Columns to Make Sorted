class Solution {
    public int minDeletionSize(String[] strs) {
        int count[]=new int[strs[0].length()];
        Arrays.fill(count,0);
        for(int i=0;i<strs.length-1;i++){
            for(int j=0;j<strs[i].length();j++){
                if(strs[i].charAt(j)>strs[i+1].charAt(j))
                    count[j]=1;
            }
        }
        int c=0;
        for(int i=0;i<count.length;i++){
            c=c+count[i];
        }
        return c;
    }
}
