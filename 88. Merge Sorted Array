class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int i=m-1;
        int j=n-1;
        int k=(m+n)-1;
        for(;i>=0&&j>=0;)
        {
            if(nums1[i]>nums2[j])
            {
                nums1[k]=nums1[i];
                    k--;
                i--;
            }
            else 
            {
                nums1[k]=nums2[j];
                k--;
                j--;
            }
        }
        while(i>=0)
            nums1[k--]=nums1[i--];
        while(j>=0)
            nums1[k--]=nums2[j--];
        for(int p=0;p<(m+n)-1;p++)
        {
            System.out.print(nums1[p]);
        }
    }
}
